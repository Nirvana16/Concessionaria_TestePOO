<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.iff.pooa20162.testepoo">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/icone"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CadastroPropActivity" />
        <activity android:name=".activity.CadastroVeicActivity" />
        <activity
            android:name=".activity.ListaPropActivity"
            android:label="@string/title_activity_lista_propri"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.ListaVeicActivity"
            android:label="@string/title_activity_lista_vei"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />


    </application>

</manifest>